<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <!-- CSS (load bootstrap from a CDN) -->
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css">
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js"></script>

  <title>Something wrong</title>
</head>
<body>
  <div class="container mt-3">
    <p class="h1">Something wrong</p>
  <% if (messages && messages.statusCode) { %>
    <h2><%= messages.statusCode %></h2>
  <% } %>

  <% if (messages && messages.statusCodeDetails) { %>
    <p><%= messages.statusCodeDetails %></p>
  <% } %>
    
    <% if (messages && messages.error && messages.error.length > 0) { %>
      <div style="color: red;">
        <% messages.error.forEach(error => { %>
            <p>* <%= error %></p>
        <% }); %>
      </div>
    <% } %>
    
    <% if (messages && messages.warning && messages.warning.length > 0) { %>
      <div style="color: rgba(255, 153, 0, 0.805);">
        <% messages.warning.forEach(warning => { %>
          <p>* <%= warning %></p>
        <% }); %>
      </div>
    <% } %>

<a href="<%= redirectURL %>"><button class="btn btn-secondary mt-3">OK</button></a>

  </div>
</body>
</html>